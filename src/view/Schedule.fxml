<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<AnchorPane prefHeight="480.0" prefWidth="960.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Schedule">
   <children>
      <Group>
         <children>
            <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#d3eaff" height="480.0" opacity="0.5" stroke="BLACK" strokeType="INSIDE" strokeWidth="3.0" width="960.0" />
            <HBox layoutX="20.0" layoutY="433.0" prefHeight="30.0" prefWidth="922.0" spacing="20.0">
               <children>
                  <Button fx:id="addButton" mnemonicParsing="false" onAction="#onAddButton" text="Add" />
                  <Button fx:id="updateButton" mnemonicParsing="false" onAction="#onUpdateButton" text="Update" />
                  <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteButton" text="Delete" />
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <ToggleButton fx:id="toggleViewButton" mnemonicParsing="false" onAction="#onToggleViewButton" text="View appointments" />
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <Button fx:id="reportsButton" mnemonicParsing="false" onAction="#onReportsButton" text="Reports" />
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <Button fx:id="logOutButton" alignment="CENTER" mnemonicParsing="false" onAction="#onLogOutButton" text="Log out" textAlignment="CENTER">
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox layoutX="543.0" layoutY="13.0" prefHeight="20.0" prefWidth="408.0" spacing="20.0">
               <children>
                  <RadioButton fx:id="appointmentByMonthRadio" mnemonicParsing="false" onAction="#onMonthToggle" prefHeight="17.0" prefWidth="152.0" selected="true" text="Appointments by month" />
                  <RadioButton fx:id="appointmentByWeekRadio" mnemonicParsing="false" onAction="#onWeekToggle" text="Appointments by week" />
               </children>
            </HBox>
            <HBox layoutX="20.0" layoutY="14.0" prefHeight="17.0" prefWidth="523.0" spacing="10.0">
               <children>
                  <Label fx:id="timeZoneDescLabel" text="Time Zone:" />
                  <Label fx:id="timeZoneLabel" text="Time Zone string" />
                  <Separator orientation="VERTICAL" prefHeight="200.0" />
                  <Label fx:id="serverTimeZoneDescLabel" text="Server Time Zone:" />
                  <Label fx:id="serverTimeZoneLabel" text="Server Time Zone string" />
               </children>
            </HBox>
         </children>
      </Group>
      <TableView fx:id="appointmentTableView" layoutX="4.0" layoutY="50.0" onMouseClicked="#onAppointmentTableViewClicked" prefHeight="365.0" prefWidth="952.0">
        <columns>
          <TableColumn fx:id="appointmentIdCol_a" prefWidth="105.0" sortable="false" text="Appointment_ID" />
          <TableColumn fx:id="titleCol_a" prefWidth="70.0" sortable="false" text="Title" />
            <TableColumn fx:id="descriptionCol_a" prefWidth="130.0" sortable="false" text="Description" />
            <TableColumn fx:id="locationCol_a" prefWidth="81.0" sortable="false" text="Location" />
            <TableColumn fx:id="contactCol_a" prefWidth="78.0" sortable="false" text="Contact" />
            <TableColumn fx:id="typeCol_a" prefWidth="71.0" sortable="false" text="Type" />
            <TableColumn fx:id="startCol_a" minWidth="9.0" prefWidth="129.0" text="Start" />
            <TableColumn fx:id="endCol_a" prefWidth="117.0" text="End" />
            <TableColumn fx:id="customerIdCol_a" minWidth="0.0" prefWidth="93.0" sortable="false" text="Customer_ID" />
            <TableColumn fx:id="userIdCol_a" prefWidth="77.0" sortable="false" text="User_ID" />
        </columns>
      </TableView>
      <TableView fx:id="customerTableView" disable="true" layoutX="4.0" layoutY="50.0" onMouseClicked="#onCustomerTableViewClicked" prefHeight="365.0" prefWidth="952.0" visible="false">
         <columns>
            <TableColumn fx:id="customerIdCol_c" text="Customer_ID" />
            <TableColumn fx:id="customerNameCol_c" prefWidth="130.0" text="Customer_Name" />
            <TableColumn fx:id="addressCol_c" prefWidth="160.0" text="Address" />
            <TableColumn fx:id="postalCodeCol_c" prefWidth="91.0" text="Postal_Code" />
            <TableColumn fx:id="phoneCol_c" prefWidth="98.0" text="Phone" />
            <TableColumn fx:id="createDateCol_c" text="Create_Date" />
            <TableColumn fx:id="createdByCol_c" minWidth="0.0" prefWidth="61.0" text="Created_By" />
            <TableColumn fx:id="lastUpdateCol_c" prefWidth="65.0" text="Last_Update" />
            <TableColumn fx:id="lastUpdatedByCol_c" prefWidth="103.0" text="Last_Updated_By" />
            <TableColumn fx:id="divisionIdCol_c" prefWidth="83.0" text="Division_ID" />
         </columns>
      </TableView>
   </children>
</AnchorPane>
